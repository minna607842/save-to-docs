{
  "manifest_version": 3,
  "name": "Save to Google Docs",
  "version": "1.0",
  "description": "Save web pages to Google Docs",
  "permissions": [
    "activeTab",
    "scripting",
    "identity",
    "tabs",
    "storage",
    "commands"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "oauth2": {
    "client_id": "34956715898-v67db0j317lbepd7fhscbngttdkt9h1n.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/documents"
    ]
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Save to Google Docs"
    }
  }
}